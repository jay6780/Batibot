{
  "version": 2,
  "functions": {
    "api/scrape.js": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/scrape-videos",
      "dest": "/api/scrape.js"
    },
    {
      "src": "/health",
      "dest": "/api/scrape.js"
    },
    {
      "src": "/",
      "dest": "/api/scrape.js"
    }
  ]
}